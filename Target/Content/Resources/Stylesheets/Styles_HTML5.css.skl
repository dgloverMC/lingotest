	ul.off-canvas-list a.selected
	{
		background-color: #ad0f5b;
		color: white;
		text-transform: uppercase;
		font-weight: inherit;
		font-size: 15px;
	}

	ul.off-canvas-list ul > li > a::before
	{
		font-family: 'Signavicons';
		padding-right: 5px;
		content: '砻קยด9ตว融';
	}

	ul.off-canvas-list li a
	{
		border: none;
	}

	li.js-drilldown-back
	{
		background-color: #fff;
		color: inherit;
	}

	li.js-drilldown-back:active
	{
		background-color: #fff;
		color: inherit;
	}

	.off-canvas.position-right.is-open
	{
		color: #333;
	}
}

