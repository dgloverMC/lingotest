/**
 * GHColors theme by Avi Aryan (http://aviaryan.in)
 * Inspired by Github syntax coloring
 */

code[class*='language-'],
pre[class*='language-']
{
	color: #393a34;
	font-family: 'Consolas', 'Bitstream Vera Sans Mono', 'Courier New', Courier, monospace;
	direction: ltr;
	text-align: left;
	white-space: pre;
	word-spacing: normal;
	word-break: normal;
	font-size: 0.95em;
	line-height: 1.2em;
	-moz-tab-size: 4;
	-o-tab-size: 4;
	tab-size: 4;
	-webkit-hyphens: none;
	-moz-hyphens: none;
	-ms-hyphens: none;
	hyphens: none;
}

pre::-moz-selection[class*='language-'],
pre[class*='language-'] ::-moz-selection,
code::-moz-selection[class*='language-'],
code[class*='language-'] ::-moz-selection
{
	background: #b3d4fc;
}

pre::selection[class*='language-'],
pre[class*='language-'] ::selection,
code::selection[class*='language-'],
code[class*='language-'] ::selection
{
	background: #b3d4fc;
}

/* Code blocks */

pre[class*='language-']
{
	padding: 1em;
	margin: 0.5em 0;
	overflow: auto;
	border: 1px solid #eee;
	background-color: white;
}

:not(pre) > code[class*="language-"],
pre[class*="language-"]
{
	
}

/* Inline code */

:not(pre) > code[class*='language-']
{
	padding: 0.2em;
	padding-top: 1px;
	padding-bottom: 1px;
	background: #f8f8f8;
	border: 1px solid #eee;
}

.token.comment,
.token.prolog,
.token.doctype,
.token.cdata
{
	color: #999988;
	font-style: italic;
}

.token.namespace
{
	opacity: 0.7;
}

.token.string,
.token.attr-value
{
	color: #e3116c;
}

.token.punctuation,
.token.operator
{
	color: #393a34;
	/* no highlight */
}

.token.entity,
.token.url,
.token.symbol,
.token.number,
.token.boolean,
.token.variable,
.token.constant,
.token.property,
.token.regex,
.token.inserted
{
	color: #36acaa;
}

.token.atrule,
.token.keyword,
.token.attr-name,
.language-autohotkey .token.selector
{
	color: #00a4db;
}

.token.function,
.token.deleted,
.language-autohotkey .token.tag
{
	color: #9a050f;
}

.token.tag,
.token.selector,
.language-autohotkey .token.keyword
{
	color: #00009f;
}

.token.important,
.token.function,
.token.bold
{
	font-weight: bold;
}

.token.italic
{
	font-style: italic;
}

